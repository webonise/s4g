<h1>Listing business users</h1>

<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

  <% @business_users.each do |business_user| %>
      <tr>
        <td><%= business_user.first_name %></td>
        <td><%= business_user.last_name %></td>
        <td><%= link_to 'Show', business_user %></td>
        <td><%= link_to 'Edit', edit_user_path(business_user) %></td>
        <td><%= link_to 'Destroy', business_user, method: :delete,
                        data: { confirm: 'Are you sure?' } %></td>
      </tr>
  <% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>