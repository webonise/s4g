<% provide(:title, 'Sign up') %>

<div class="row">
  <div class="span5 offset2">
    <h1> User Sign up</h1>

    <%= form_for Users_path(@user), :method => :post, :html => { :class => 'form-horizontal' }  do |f| %>

        <%#= render 'error_messages', object: f.object %>

        <%= render 'shared/sign_up', object: f.object %>

        <div class="control-group">

          <div class="controls">
            <%= f.submit "Sign Up", class: "btn btn-large btn-primary" %>
          </div>
        </div>



        <!--</div>-->
    <% end %>
  </div>

</div>